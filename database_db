CREATE DATABASE IF NOT EXISTS poi_database;USE poi_database;CREATE TABLE poi_data (    id INT AUTO_INCREMENT PRIMARY KEY,    region_name VARCHAR(100),    poi_type VARCHAR(100),    latitude DECIMAL(10,7),    longitude DECIMAL(10,7),    display_name VARCHAR(255),    address VARCHAR(255),    name VARCHAR(100),    road VARCHAR(100),    house_number VARCHAR(20),    post_code VARCHAR(20),    suburb VARCHAR(100),    city VARCHAR(100),    state VARCHAR(100),    country VARCHAR(100));SHOW TABLES;tasklist /FI "PID eq <16004>"